
.qrcode {
  height: 420rpx;
  display: flex;
  flex-direction: column;
  justify-content: flex-end;
  align-items: center;
}

.qrcode > canvas {
  width: 420rpx;
  height: 420rpx;
}
.no-from image{
  width: 250rpx;
  height: 200rpx;
}
.no-from p{
  font-size: xx-small;
  color: #888888;
}
.cu-card>.cu-item {
  margin: 15rpx;
}
.info{
  display: flex;
  flex-wrap: wrap;
  padding: 10rpx;
}
.text-img{
  width: 230rpx;
  height: 230rpx;
  margin: 1px;
}
.card-footer{
  display: flex;  
  justify-content: space-between;
  align-items: center;
  padding: 10rpx;
}
.loupan{
  width: 100%;
  display: inline-block;
  white-space: nowrap;
  overflow: hidden;
  text-overflow: ellipsis;
}
.order{
  margin: 10rpx 15rpx;
  border-radius: 10px;
}
.order-centent{
  width: 100%;
  padding: 10rpx;
  display: flex;
  flex-direction: column;
}
.steps-order{
  display: flex;
  flex-direction: column;
  justify-content: center;
  align-items: center;
}
.order-bottom{
  margin-bottom: 150rpx;
}
.order-centent .cu-form-group view{
  width: 180rpx;
  text-align: left;
}
.title-line{
  border-bottom: 3rpx dashed #F6F6F6;
  border-top-left-radius: 15rpx;
  border-top-right-radius: 15rpx;
}
.box-line{
  border-bottom: 3rpx dashed #F6F6F6;
  position: relative;
}
.box-line::before{
  position: absolute;
  content: "";
  width: 20px;
  height: 20px;
  background: #F6F6F6;
  border-radius: 100%;
  left: 0;
  bottom: -40rpx;
  transform: translate(-50%, -50%);
}
.box-line::after{
  position: absolute;
  content: "";
  width: 20px;
  height: 20px;
  background: #F6F6F6;
  border-radius: 100%;
  right: 0;
  bottom: -40rpx;
  transform: translate(50%, -50%)
}
.order-centent{
  padding: 10rpx;
  display: flex;
  flex-direction: column;
}
.footer-tips{
  margin-bottom: 50rpx;
  display: flex;
  justify-content: center;
}
.list .title{
  width: 100%;
  display: flex;
  justify-content: space-between;
  align-items: center;
  border-bottom: 1px solid #F3F4F7;
}
.list .title .ticket-detail-container{
  margin-right: 5px;
}
.list .title .left{
  height: 100rpx;
  display: flex;
  align-items: center;
}
.list .title .left l-icon{
  font-size: 18px;
  font-weight: 200px;
  margin-left: 10px;
}
.list .title .left .text{
  font-size: 18px;
  font-weight: 200px;
  margin-left: 10px;
}
.list .text-h3{
  font-size: 18px;
  font-weight: 200px;
  margin-left: 10px;
  text-align: center;
}
.list .content{
  font-size: 16px;
  line-height: 20px;
  margin: 20rpx;
}
.right-img {
  width: 80rpx;
  height: 80rpx;
}
.bottom-line{
  border-top: 3rpx dashed #F6F6F6;
  position: relative;
}
.footer-line::before{
  position: absolute;
  content: "";
  width: 20px;
  height: 20px;
  background: #F6F6F6;
  border-radius: 100%;
  left: 0rpx;
  top: 0rpx;
  transform: translate(-50%, -50%);
}
.footer-line::after{
  position: absolute;
  content: "";
  width: 20px;
  height: 20px;
  background: #F6F6F6;
  border-radius: 100%;
  right: 0rpx;
  top: 0rpx;
  transform: translate(50%, -50%)
}
.tips{
	display: flex;
	flex-direction: column;
	margin: 5rpx;
	background-color: #FFEAC6;
	border-radius: 10px;
	color: #CD5107;
	padding: 20rpx;
}
.tips .title{
	text-align: center;
	margin: 10rpx 0;
}
.order-foot{
  width: 96%;
  margin: 0 2%;

}
.order-foot view:first-child{
  border-top-left-radius: 15rpx;
}
.order-foot view:last-child{
  border-top-right-radius: 15rpx;
}