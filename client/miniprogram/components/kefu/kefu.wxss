.kefu{
	display: flex;
	justify-content: center;
	align-items: center;
	position: fixed;
	bottom: 10%;
	right: 5%;
	width: 85rpx;
	height: 85rpx;
	border-radius: 50%;
	/* box-shadow:0px 0px 1.5px #888888; */
}
.kefu .container {
	display: flex;
	flex-direction: row;
	align-items: center;
}
movable-area{
  width: 100%;
  height:100%;
  /*设置透明不影响后面的操作*/
  pointer-events: none;
  position:fixed;
  text-align: center;
  right:0px;
  top:0px;
  background: none;
  }